{
  "name": "@accosine/nepodory-leitmotif",
  "version": "0.0.0-development",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "node ./tests/jest.js",
    "test:ci": "node ./tests/jest.js --maxWorkers=2",
    "export": "next build && next export",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.13.13",
    "@mdx-js/runtime": "1.6.22",
    "@testing-library/dom": "^7.30.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "babel-jest": "^26.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^17.4.2",
    "styled-jsx": "^3.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "styled-jsx/babel"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/accosine/next-leitmotif-podcast-story.git"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
